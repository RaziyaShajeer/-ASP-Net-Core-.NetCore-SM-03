@model IEnumerable<MVCassessment.Models.Vehicle>
<h2 class="text-success">
    Welcome, Admin -  @TempData["Message"] !
</h2>
<div>
    <h3 class="text-warning">Add Vehicle Page</h3>
</div>
    <div class="card">
    <a asp-action="Logout" asp-controller="Account" class="btn btn-danger">Logout</a>

        <a asp-action="Create" asp-controller="Vehicle" class="btn btn-info">Add New Vehicle</a>
    </div>
   <div>

  
    <table class="table table-bordered">
        <thead class="table-dark">
            <tr class="table-striped">
            <th>VehicleModel</th>
            <th>Category</th>
            <th>RentPerDay</th>
            <th>IsAvailableVehicle</th>

            </tr>
        </thead>
        <tbody>
            @foreach(var vehicle in Model)
            {
            <tr>
                <td>@vehicle.VehicleModel</td>
               <td>@vehicle.Category</td>

                 <td>@vehicle.RentPerDay</td>

                <td>@vehicle.IsAvailableVehicle</td>

            </tr>
        }
        </tbody>
    </table>
</div>